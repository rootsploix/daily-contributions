{
  "name": "rootsploix-daily-contributions",
  "version": "4.2.0",
  "description": "Advanced Enterprise Cybersecurity & Blockchain Security Platform",
  "main": "src/features/advanced-security.js",
  "scripts": {
    "start": "node src/features/advanced-security.js",
    "blockchain": "node src/blockchain/security-analyzer.js",
    "performance": "python src/features/performance-optimizer.py",
    "ai-detect": "python src/ai/threat-detection.py",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/**/*.js && flake8 src/**/*.py",
    "format": "prettier --write src/**/*.js && black src/**/*.py",
    "security-scan": "npm audit && bandit -r src/",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rootsploix/daily-contributions.git"
  },
  "keywords": [
    "cybersecurity",
    "blockchain",
    "ai",
    "threat-detection",
    "smart-contracts",
    "defi",
    "security-analysis",
    "vulnerability-scanner",
    "performance-optimization",
    "crypto-mining"
  ],
  "author": "RootsploiX Security Research Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rootsploix/daily-contributions/issues"
  },
  "homepage": "https://github.com/rootsploix/daily-contributions#readme",
  "dependencies": {
    "ethers": "^6.7.1",
    "crypto": "^1.0.1",
    "ws": "^8.14.2",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}